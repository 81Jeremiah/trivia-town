<%= form_for @quiz do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>


    <%= f.fields_for :question_and_answers do |q| %>
        <%= q.label :question %>
        <%= q.text_field :question %><br>
        <%= q.label :answer %>
        <%= q.text_field :answer %><br>
    <% end %>



    <%= f.submit %>
<% end %>
