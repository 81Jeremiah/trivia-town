Take the <%= @quiz.name %> Quiz!!

<% questions = ["1?", "2?", "3", "4?", "5?"] %>



<%= form_for @game do |f| %>
<%= f.hidden_field :quiz_id %>
<% @quiz.question_and_answers.each do |qa| %>
Question: <%= qa.question %>
         <%= f.fields_for :guesses do |g| %>
           <%= g.label :guess, "Answer" %>
           <%= g.text_field :guess %>
        <% end %>

<% end %>
<%= f.submit %>
<% end %>